#include <stdio.h>
#include <stdlib.h>
#include "biblioteca.h"

int enteros(int a);

int main(void) {
	setbuf(stdout, NULL);

	int numero;

	printf("Ingrese un numero: ");
	scanf("%d", &numero);

	numero = enteros(numero);

	printf("El numero es: %d", numero);

	return EXIT_SUCCESS;
}



